  <!--修改密码静态框-->
    <div class="modal fade" id="myPassword" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
      <div class="modal-dialog" style="color:#a2d3f9">
        <div class="modal-content">
          <div class="modal-header">
            <div class="input" style="width: 500px;text-align: center;color: #26354c;">
              <span>原密码:&nbsp;&nbsp;&nbsp;</span><input type="text" id="oldPassword" name="oldPassword" 
               ng-model="params.oldPassword" ><br><br>
              <span>新密码:&nbsp;&nbsp;&nbsp;&nbsp;</span><input type="text" id="newPassword" name="newPassword" ng-model="params.newPassword">
              <br><br>
              <span>确认密码:&nbsp;</span><input type="text"  id ="confirmPassword" name="confirmPassword" ng-model="params.confirmPassword">
              <br><br>    
            </div>
      <div  style="text-align: center;color: #26354c;">
      <button  align="center" type="button" class="btnself" id="lhUpdate" ng-disabled="form4.$invalid" data-toggle="modal" ng-click="saveInfo(params)">保存</button>
      <button type="button" class="btnself" data-toggle="modal" data-dismiss="modal" ng-click="cancel();">取消</button>
      </div> 







